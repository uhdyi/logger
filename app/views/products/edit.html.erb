<h1>Edit Part Number</h1>


<% form_for(@product) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :part_number %><br />
    <%= f.text_field :part_number, :disabled => true %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', products_path %>
